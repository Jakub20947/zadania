#include <iostream>
#include <string>

#include <fstream>
using namespace std;

class plik {
    ifstream input;
    string nazwa_pliku = "trojki.txt";
    
    public:
        void otworzPlik();
        void zamknijPlik();
        void wczytajPlik();
        void oblicz_sume();
};

void plik::otworzPlik() {
    input.open( nazwa_pliku.c_str(), std::ios::in );
if( !input.is_open() ) {
    cout << endl << "Blad otwarcia pliku wejsciowego." << endl;
    }
}

void plik::zamknijPlik() {
    input.close();
}

void plik::wczytajPlik() {
  int i, liczba1, liczba2, liczba3; 
     for (i=1; i<=1000; i++)
 {
 scanf("%i %i %i\n",&liczba1, &liczba2, &liczba3);
 } 

		
}

void plik::oblicz_sume(){
	int suma_cyfr;
	int a;
	int suma;
	{int suma=0;
 	while (a>0)
 	{suma=suma+a%10;a=a/10;}
	 return(suma);
} 
 
 if ((suma_cyfr(liczba1)+suma_cyfr(liczba2))==liczba3)){
 	 cout<< liczba1 <<liczba2 <<liczba3;

 }
 
}


int main(int argc, char** argv) {
    plik p;
    p.otworzPlik();
    p.wczytajPlik();
    p.oblicz_sume();
    p.zamknijPlik();  
    return 0;
}
